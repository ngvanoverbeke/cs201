        } else if (!config.custom_css.match(/^https?:/)) {
          config.custom_css = config.origin.replace(/\/+$/, "") + "/"
            + config.custom_css.replace(/^\/+/, "");
        }

        var expression = /^https?:\/\/\w[\.\w\-]*(:[0-9]+)?[^\s<>'"]*$/;
        var regex = new RegExp(expression);
        if (config.custom_css.match(regex)) {
